<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="cameraType" format="enum">
        <enum name="back" value="0" />
        <enum name="front" value="1" />
    </attr>

    <declare-styleable name="CodeReaderView">
        <attr name="cameraType" />
        <attr name="isFlipCameraButtonVisible" format="boolean" />
        <attr name="flipCameraDrawable" format="reference" />
        <attr name="flipCameraDrawablePadding" format="dimension" />
    </declare-styleable>

</resources>